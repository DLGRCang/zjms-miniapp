<!--pages/medical/pages/oldAge/oldAge.wxml-->
<cu-custom bgColor="bgColor" isBack="{{true}}">
  <view slot="content" class="text-white text-xl text-black text-bold">高龄补助</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/gaolingbutie.png" style="width:100%;height:396rpx;"></image>
<button bindtap="oldAgeSubsidy" class="button">在线申请</button>

<view class="margin-lr-lg padding-tb-lg">

  <dot-title title="相关政策" color="title-color"></dot-title>
  <view class="padding-lr-sm padding-tb">
    <item-image-text wx:for="{{dataList}}" wx:key="item" obj="{{item}}" isImg="{{false}}"
      itemTitle="{{item.info_content}}" isDate="true" date="{{item.publishdate}}"></item-image-text>
  </view>
</view>