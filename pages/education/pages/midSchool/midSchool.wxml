<cu-custom bgColor="bgColor" isBack="{{true}}">
  <view slot="content" class="text-white text-xl text-black text-bold">中小学</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/zxx.png" style="width:100%;height:396rpx;"></image>
<view class="margin-lr-lg">
  <dot-title title="小学学校" color="color" isMore="true" sign='{{infotypeid}}'></dot-title>
  <item-image-text wx:for="{{dataList}}" imgUrl="{{item.info_photos}}" itemTitle="{{item.info_content}}"
    itemInfo="{{item.info_summary}}" isImg="true" isTab="true" itemTabs="{{item.infotypePname}}" isLine="true">
  </item-image-text>
  <dot-title title="中学学校" color="color" isMore="true" sign='{{infotypeid1}}'></dot-title>
  <item-image-text wx:for="{{dataList1}}" imgUrl="{{item.info_photos}}" itemTitle="{{item.info_content}}"
    itemInfo="{{item.info_summary}}" isImg="true" isTab="true" itemTabs="{{item.infotypePname}}" isLine="true">
  </item-image-text>
</view>