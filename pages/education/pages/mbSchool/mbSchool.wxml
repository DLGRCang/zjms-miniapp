<cu-custom bgColor="bgColor" isBack="{{true}}">
  <view slot="content" class="text-white text-xl text-black text-bold">校外培训机构</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/minbanxuexiao.png" style="width:100%;height:396rpx;"></image>
<button class="button">普惠性民校认定</button>
<view class="margin-lr-lg padding-tb-lg">
  <dot-title title="校外培训机构" color="color" isMore="true" sign="{{infotypeid}}" type="3"></dot-title>


  <item-image-text wx:for="{{dataList}}" obj="{{item}}" imgUrl="{{item.info_photos}}" itemTitle="{{item.info_content}}"
    itemInfo="{{item.info_summary}}" isImg="true" isTab="true" itemTabs="{{item.infotypePname}}" isLine="true">
  </item-image-text>


  <dot-title title="相关规则" color="color" isMore="true" sign="{{infotypeid1}}" type="1"></dot-title>


  <item-image-text wx:for="{{dataList1}}" obj="{{item}}" itemTitle="{{item.info_content}}" isImg="{{false}}"
    isDate="true" date="{{item.putlishdate}}" isLine="true">
  </item-image-text>

</view>