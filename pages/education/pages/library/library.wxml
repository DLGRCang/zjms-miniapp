<cu-custom bgColor="bgColor" isBack="{{true}}">
  <view slot="content" class="text-white text-xl text-black text-bold">图书馆</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/tushuguan.png" style="width:100%;height:396rpx;"></image>
<view class="margin-lr-lg padding-tb-lg">
  <dot-title title="旗图书馆" color="color" isMore="true"></dot-title>
  <view class="flex justify-between">
    <view wx:for="{{3}}" wx:key="item">
      <view><image src="{{imgUrl}}/image/tushuguan.png" style="height:276rpx;width:200rpx;"></image></view>
      <view class="margin-tb-xs">没有此人 更高…</view>
      <view class="text-sm text-grey">借阅 203 次</view>
    </view>
  </view>

  <dot-title title="民委图书馆" color="color" isMore="true"></dot-title>
  <view class="flex justify-between">
    <view wx:for="{{3}}" wx:key="item">
      <view><image src="{{imgUrl}}/image/tushuguan.png" style="height:276rpx;width:200rpx;"></image></view>
      <view class="margin-tb-xs">没有此人 更高…</view>
      <view class="text-sm text-grey">借阅 203 次</view>
    </view>
  </view>
</view>