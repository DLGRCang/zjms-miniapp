<!--pages/food/pages/cultureFood/cultureFood.wxml-->
<cu-custom bgColor="bgColor" isBack="{{true}}">
	<view slot="content" class="text-white text-xl text-black text-bold">{{tit}}</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/yinshiwenhua.png" style="width:100%"></image>
<view class="padding-lr-sm padding-tb">
	<item-image-text wx:for="{{dataList}}" obj="{{item}}" imgUrl="{{item.info_photos}}" itemTitle="{{item.info_content}}" isDate="true"
		date="{{item.publishdate}}"></item-image-text>
</view>