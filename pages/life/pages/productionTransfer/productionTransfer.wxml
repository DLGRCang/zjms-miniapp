<!--pages/life/pages/productionTransfer/productionTransfer.wxml-->
<cu-custom bgColor="bgColor" isBack="{{true}}">
	<view slot="content" class="text-white text-xl text-black text-bold">生产转让</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/shengchanzhuanrang.png" style="width:100%"></image>
<button class="button">发布转让信息</button>
<view class="padding padding-tb-xl">
	<item-image-text wx:for="{{dataList}}" imgUrl="{{item.info_photos}}" itemTitle="{{item.info_content}}" isDate="true"
		date="{{item.publishdate}}"></item-image-text>
</view>