<!--pages/life/pages/moveCar/moveCar.wxml-->
<cu-custom bgColor="bgColor" isBack="{{true}}">
  <view slot="content" class="text-white text-xl text-black text-bold">代驾挪车</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/daijianuoche.png" style="width:100%;height:396rpx;"></image>

<view class="padding-lr">
<dot-title title="附近公司" color="color"></dot-title>
<item-image-text wx:for="{{dataList}}" obj="{{item}}" imgUrl="{{item.info_photos}}" itemTitle="{{item.info_content}}"
  itemInfo="{{item.info_summary}}" isImg="true" isTab="true" itemTabs="{{item.infoTypeLabel}}" isLine="true">
</item-image-text>
</view>
