/* pages/part/pages/register/register.wxss */
.bgColor {
  background-color: #B81C24;
  color: #ffffff;
}

.color {
  color: #B81C24;
}

.list {
  width: 10rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-left: 10rpx solid;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  margin-top: 6rpx;
}

.line {
  border-bottom: 1rpx solid #F2F2F2;
  height: 2rpx;
  width: 100%;
  margin: 30rpx auto;
}
.textInfo {
  display: inline-block;
  white-space: nowrap;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}
.area input{
  width: 100rpx;
}
