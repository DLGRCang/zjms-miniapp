/* pages/part/pages/sHMore/sHMore.wxss */
.bgColor {
  background-color: #B81C24;
  color: #ffffff;
}

.color {
  color: #B81C24;
}

.nav .cu-item {
	height: 68rpx;
	display: inline-block;
	line-height: 68rpx;
	margin: 0 30rpx;
	padding: 0 20rpx;
}

.cu-item {
  width: 180rpx;
  height: 64rpx;
}

.tab {
  color: #ffffff;
  background-color: #B81C24;
  border-radius: 15rpx;
}

.contInfo{
  width: 10%;
}








.images {
  width: 100rpx;
  height: 100rpx;
  border: 6rpx solid #ffffff;
  border-radius: 50%;
}

.userCenter {
  width: 140rpx;
  height: 64rpx;
  border: 1rpx solid #ffffff;
  border-radius: 10rpx;
  line-height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #ffffff;
  font-size: 26rpx;
  margin-left: 120rpx;
  margin-top: 30rpx;
}

.title {
  height: 76rpx;
  border: 1rpx solid #ffffff;
  background: #FFFFFF;
  box-shadow: 0px 0px 6rpx 0px rgba(0, 0, 0, 0.15);
  border-radius: 4rpx;
  margin-top: -70rpx;
}
.list {
  width: 10rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-left: 10rpx solid ;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  margin-top: 6rpx;
}
.line{
  border-bottom: 1rpx solid #F2F2F2;
	height: 2rpx;
  width: 100%;
	margin: 30rpx auto;
}
.sign{
  position: absolute;
  width: 120rpx;
  height: 64rpx;
  border: 1rpx solid #B81C24;
  border-radius: 10rpx;
  background-color: #B81C24;
  color: #ffffff;
  line-height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  right: 30rpx;
  bottom: 20rpx;
}
.textInfo {
  display: inline-block;
  white-space: nowrap;
  width: 280rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}