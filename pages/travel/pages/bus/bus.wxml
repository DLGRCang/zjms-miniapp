<cu-custom bgColor="bgColor" isBack="{{true}}">
  <view slot="content" class="text-white text-xl text-black text-bold">公交出租</view>
</cu-custom>
<image mode="widthFix" src="{{imgUrl}}/image/gongjiaochuzu.png" style="width:100%;height:396rpx;"></image>
<view class="margin-lr-lg">
  <dot-title title="出租讯息" color="color"></dot-title>
  <view class="box">
    <view class="flex justify-between text-xl padding-sm margin-top-sm" bindtap="goTaxiInfo">
      <view class="flex">
        <view class="cuIcon-taxi text-bold" style="padding-top: 5rpx;color:#EE954A;"></view>
        <view class="padding-left-sm">出租车主</view>
      </view>
      <view class="cuIcon-right text-grey" style="padding-top: 14rpx;"></view>
    </view>
    
    <view class="line"></view>
    <view class="flex justify-between text-xl padding-sm margin-top-sm" bindtap="">
      <view class="flex">
        <view class="cuIcon-deliver text-bold" style="padding-top: 5rpx;color:#EE954A;"></view>
        <view class="padding-left-sm">在线约车</view>
      </view>
      <view class="cuIcon-right text-grey" style="padding-top: 14rpx;"></view>
    </view>
    <view class="line"></view>

    <view class="flex justify-between text-xl padding-sm margin-top-sm" bindtap="">
      <view class="flex">
        <view class="cuIcon-goods text-bold" style="padding-top: 5rpx;color:#EE954A;"></view>
        <view class="padding-left-sm">失物招领</view>
      </view>
      <view class="cuIcon-right text-grey" style="padding-top: 14rpx;"></view>
    </view>
    <view class="margin-top-xs"></view>
  </view>
  <dot-title title="公交讯息" color="color"></dot-title>
  <view class="flex justify-between text-xl padding-sm margin-top-sm" bindtap="goBus">
    <view class="flex">
      <view class="cuIcon-order text-bold" style="padding-top: 5rpx;color:#EE954A;"></view>
      <view class="padding-left-sm">公交路线</view>
    </view>
    <view class="cuIcon-right text-grey text-bold" style="padding-top: 14rpx;"></view>
  </view>
</view>
<bottom style="margin-top:auto" deptStr1="主管单位：伊金霍洛旗大数据发展局" deptStr2="内蒙古大云智通科技有限公司"></bottom>