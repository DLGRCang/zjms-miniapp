<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="content">我的预约</view>
</cu-custom>
<tab bindmyevent="selectTab" tabName="{{pageTabs}}" tvColor="color"></tab>
<view class="margin-lg">
  <view bindtap="goInfo" data-id="{{item.applyId}}" class="flex justify-between margin-lg" wx:for="{{dataList}}">
    <view class="flex">
      <view class="padding-xs">
        <view class="text-lg text-bold">
          {{item.applyType}}
        </view>
        <view class="text-sm text-grey margin-top-sm">
          {{item.applyDate}}
        </view>
      </view>
    </view>
    <view style="padding-top:30rpx">
      <text class="cuIcon-right text-grey"></text>
    </view>
  </view>

</view>